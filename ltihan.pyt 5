import math #buat import dari file lain jadi kita kaya manggil

def getarea(w, l):
    area = w * l
    return area

def getname(nm):
    print('Hello', nm)

def sayhello(): #boleh pake argumen atau tidak
    print('Hello Good morning')


width = 10
length = 5
area = width * length

print(area, 'no fuction')
print(getarea(20, getarea(5, 2)), 'is a return from getarea function')
