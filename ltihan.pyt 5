import random
print(random.triangular(10,20,30))

a = 1
b = 10
bet = 100000
x = 1
if bet > 1000000:
    if x == random.randint(a, b):
        print('you win', bet * 2)
    else:
        print('you lose')
elif bet > 10000:
    b = 10
    if x == random.randint(a, b):
        print('you win', bet * 2)
    else:
        print('you lose')
else:
    b = 3
    if x == random.randint(a, b):
        print('you win', bet * 2)
    else:
        print('you lose')